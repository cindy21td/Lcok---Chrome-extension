{
  "manifest_version": 2,

  "name": "Lcok",
  "description": "A webpage lock to increase productivity hour.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
  	"scripts": ["background.js"],
  	"persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}